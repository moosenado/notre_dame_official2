@model NotreDameReBuildOfficial.Models.PDF_Filter

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_LayoutCMS.cshtml";
    @Scripts.Render("~/Scripts/cms-menu.js");
}

<h2>Delete</h2>

<h2>Are you sure you want to delete this appointment? <em>@Model.PdfTitle from @Model.PdfUrl?</em></h2>

@using (Html.BeginForm())
{
    <input type="submit" value="Delete" />

    @Html.ActionLink("Back", "Details", new { id = Model.id })
}
