@model IEnumerable<NotreDameReBuildOfficial.Models.Event>

@{
    ViewBag.Title = "cmsEventsListing";
    Layout = "~/Views/Shared/_LayoutCMS.cshtml";
    @Scripts.Render("~/Scripts/cms-menu.js");
}

<h1>Manage Events</h1>


<div class="head-btns">

    <a href='@Url.Action("Insert", "cmsEventsListing")'>
        <span class="buttonstyle"><i class="fa fa-plus"></i>New Event</span>
    </a>
    
    @{
        Html.RenderAction("_TotalSubmittedEventsPartial", "cmsEventsListing");
    }

</div>

<div class="clear"></div>

<h3>Upcoming Events</h3>
@{
    Html.RenderAction("_UpcomingEventsPartial", "cmsEventsListing");

}


<h3>Archived Events</h3>
@{
    Html.RenderAction("_PastEventsPartial", "cmsEventsListing"); 
}