@model IEnumerable<NotreDameReBuildOfficial.Models.staff_info>

@{
    ViewBag.Title = "staffList";
}

<h1>staff Directory</h1>
<h2>Search for staff Information</h2>


<table>
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Department</th>
        </tr>
    </thead>

    @foreach (var staff in Model)
    {
        <tr>
            <td>@staff.firstname</td>
            <td>@staff.lastname</td>
            <td>@staff.department</td>
            <td>@Html.ActionLink("View", "staffInfo", new { id = staff.id })</td>
        </tr>
    }

</table>